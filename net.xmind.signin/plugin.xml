<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="net.xmind.ui.commands"
            name="%commandCategory.xmindnet.name">
      </category>
      <command
            categoryId="net.xmind.ui.commands"
            description="%command.signIn.description"
            id="net.xmind.ui.command.signin"
            name="%command.signIn.name">
      </command>
      <command
            categoryId="net.xmind.ui.commands"
            description="%command.showAccount.description"
            id="net.xmind.ui.command.showMyAccount"
            name="%command.showAccount.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="net.xmind.ui.actions.help"
            label="%actionSet.xmindnet.label"
            visible="true">
         <action
               class="net.xmind.signin.internal.actions.UpgradeActionDelegate"
               id="net.xmind.ui.upgrade"
               label="%action.upgrade.label"
               menubarPath="help/more"
               style="push"
               tooltip="%action.upgrade.tooltip">
         </action>
         <action
               class="net.xmind.signin.internal.actions.ShowAccountActionDelegate"
               definitionId="net.xmind.ui.command.showMyAccount"
               id="net.xmind.ui.showMyAccount"
               label="%action.showAccount.label"
               menubarPath="help/additions"
               style="push"
               tooltip="%action.showAccount.tooltip">
         </action>
         <action
               class="net.xmind.signin.internal.actions.SignInActionDelegate"
               definitionId="net.xmind.ui.command.signin"
               id="net.xmind.ui.signin"
               label="%action.signIn.label"
               menubarPath="help/additions"
               style="push"
               tooltip="%action.signIn.tooltip">
         </action>
         <action
               class="net.xmind.signin.internal.actions.InviteActionDelegate"
               id="net.xmind.ui.invite"
               label="%action.invite.label"
               menubarPath="help/group.xmind"
               style="push"
               tooltip="%action.invite.tooltip">
         </action>
         <action
               class="net.xmind.signin.internal.actions.FeedbackActionDelegate"
               id="net.xmind.ui.feedback"
               label="%action.feedback.label"
               menubarPath="help/group.xmind"
               style="push"
               tooltip="%action.feedback.tooltip">
         </action>
         <action
               class="net.xmind.signin.internal.actions.WelcomeActionDelegate"
               id="net.xmind.ui.welcome"
               label="%action.welcome.label"
               menubarPath="help/group.welcome"
               style="push"
               tooltip="%action.welcome.tooltip">
         </action>
         <action
               class="net.xmind.signin.internal.actions.HelpActionDelegate"
               id="net.xmind.ui.help"
               label="%action.help.label"
               menubarPath="help/group.main"
               style="push"
               tooltip="%action.help.tooltip">
         </action>
      </actionSet>
   </extension>

</plugin>
